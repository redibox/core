{
  "name": "redibox",
  "description": "Redis connection and PUBSUB subscription manager for node. Built for performance, powered by ioredis.",
  "version": "2.2.0",
  "main": "lib/index.js",
  "author": "Mike Diarmid",
  "repository": "redibox/core",
  "license": "MIT",
  "engines": {
    "node": ">=6.0"
  },
  "keywords": [
    "redis",
    "redis-cluster",
    "cluster",
    "scheduler",
    "scheduling",
    "waterline",
    "sails",
    "cache",
    "throttle",
    "throttling",
    "timeseries",
    "autocomplete",
    "indexes",
    "indices",
    "lock",
    "redlock",
    "lua",
    "ioredis"
  ],
  "scripts": {
    "build": "babel src --out-dir lib",
    "build-watch": "babel src --watch --out-dir lib",
    "example": "cd example && ./../node_modules/.bin/babel-node app.js",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prepublish": "npm run lint && npm run build",
    "lint": "./node_modules/.bin/eslint ./src",
    "test": "./node_modules/.bin/babel-node -- ./node_modules/.bin/babel-istanbul cover --report lcov _mocha "
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.1.0",
    "babel-istanbul": "^0.8.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-preset-es2015-node6": "^0.2.0",
    "babel-preset-stage-0": "^6.5.0",
    "benchmark": "^2.1.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5",
    "sinon": "^1.17.4"
  },
  "dependencies": {
    "bluebird": "^3.4.1",
    "cuid": "^1.3.8",
    "eventemitter3": "^1.2.0",
    "ioredis": "^2.2.0",
    "json-simple": "^0.9.6",
    "winston": "^2.2.0"
  }
}
